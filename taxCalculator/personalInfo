namespace TaxCalculator
{
    public class PersonalInfo
    {
        public string Name { get; set; }
        public DateTime DOB { get; set; }
        public char Gender { get; set; }

        public PersonalInfo(string name, DateTime dob, char gender)
        {
            Name = name;
            DOB = dob;
            Gender = gender;
        }
    }
}
